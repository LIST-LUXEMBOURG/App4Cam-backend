{
  "name": "app4cam-backend",
  "version": "1.1.1-next",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "node scripts/save-commit-hash-to-file.js && nest build",
    "format": "prettier --write --ignore-unknown .",
    "prepare": "husky install",
    "start": "node scripts/save-commit-hash-to-file.js && cross-env NODE_ENV=development nest start",
    "start:dev": "node scripts/save-commit-hash-to-file.js && cross-env NODE_ENV=development nest start --watch",
    "start:debug": "node scripts/save-commit-hash-to-file.js && cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=e2e-tests jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "cross-env NODE_ENV=e2e-tests jest --config ./test/jest-e2e.json --watch"
  },
  "dependencies": {
    "@nestjs/common": "9.3.9",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.3.9",
    "@nestjs/platform-express": "9.3.9",
    "@nestjs/schedule": "2.2.0",
    "@nestjs/swagger": "6.2.1",
    "archiver": "5.3.1",
    "axios": "1.3.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "esbuild": "0.17.10",
    "luxon": "3.2.1",
    "path": "0.12.7",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.2.0",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.9",
    "@types/adm-zip": "0.5.0",
    "@types/archiver": "5.3.1",
    "@types/cron": "2.0.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.4.0",
    "@types/node": "18.14.0",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "cross-env": "7.0.3",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "lint-staged": "12.5.0",
    "msw": "1.0.1",
    "prettier": "2.8.4",
    "rimraf": "4.1.2",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.8.4"
  },
  "lint-staged": {
    "src/**/*.ts": "eslint",
    "**/*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": ">=16"
  }
}
