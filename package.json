{
  "name": "app4cam-backend",
  "version": "1.1.1-next",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "node scripts/save-commit-hash-to-file.js && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepare": "husky install",
    "start": "node scripts/save-commit-hash-to-file.js && cross-env NODE_ENV=development nest start",
    "start:dev": "node scripts/save-commit-hash-to-file.js && cross-env NODE_ENV=development nest start --watch",
    "start:debug": "node scripts/save-commit-hash-to-file.js && cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=e2e-tests jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.3",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.3",
    "@nestjs/platform-express": "^8.4.3",
    "@nestjs/schedule": "^1.1.0",
    "archiver": "^5.3.1",
    "axios": "^0.26.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.4",
    "@nestjs/schematics": "^8.0.9",
    "@nestjs/testing": "^8.4.3",
    "@types/adm-zip": "^0.5.0",
    "@types/archiver": "^5.3.1",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs": "^0.0.1-security",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "src/**/*.ts": "eslint",
    "**/*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": ">=16"
  }
}
