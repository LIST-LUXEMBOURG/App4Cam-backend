{
  "name": "app4cam-backend",
  "version": "3.0.0-next",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "node scripts/save-commit-hash-to-file.js && nest build",
    "format": "prettier --write --ignore-unknown .",
    "prepare": "husky install",
    "start": "node scripts/save-commit-hash-to-file.js && cross-env NODE_ENV=development nest start",
    "start:dev": "node scripts/save-commit-hash-to-file.js && cross-env NODE_ENV=development nest start --watch",
    "start:debug": "node scripts/save-commit-hash-to-file.js && cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=e2e-tests jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "cross-env NODE_ENV=e2e-tests jest --config ./test/jest-e2e.json --watch"
  },
  "dependencies": {
    "@nestjs/common": "10.2.5",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.5",
    "@nestjs/platform-express": "10.2.5",
    "@nestjs/schedule": "3.0.3",
    "@nestjs/swagger": "7.1.11",
    "archiver": "6.0.1",
    "axios": "1.5.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "luxon": "3.4.3",
    "path": "0.12.7",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.17",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.5",
    "@types/adm-zip": "0.5.1",
    "@types/archiver": "5.3.2",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.5",
    "@types/luxon": "3.4.2",
    "@types/node": "20.6.2",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "cross-env": "7.0.3",
    "esbuild": "0.19.3",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "14.0.1",
    "msw": "1.3.1",
    "prettier": "3.0.3",
    "rimraf": "5.0.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "src/**/*.ts": "eslint",
    "**/*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": ">=18"
  }
}
